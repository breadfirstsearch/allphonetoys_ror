<div class="col-lg-12 text-right">
  <%= link_to new_timing_path do %>
    <span class="new-transaction">+</span></a>
  <% end %>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Day</th>
      <th>Time</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @timings.each do |timing| %>
      <tr>
        <td><%= timing.day%></td>
        <td><%= timing.hours + ":" + timing.minutes + " " + timing.ampm + " - " +(Time.strptime(timing.hours+timing.minutes+timing.ampm,"%I%M%P")+1.hours).strftime("%I:%M %P")  %></td>
        <td>
          <div class="delete-location">
            <%= link_to edit_timing_path(timing), method: "get" do %>
              <span class="glyphicon glyphicon-edit"></span>
            <% end %>
          </div>
          <div class="delete-location">
            <%= link_to deleteTiming_path(timing), id: timing.id, method: "delete" do %>
              <span class="glyphicon glyphicon-remove"></span>
            <% end %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
