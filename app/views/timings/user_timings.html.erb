<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Day</th>
      <th>Time</th>
    </tr>
  </thead>

  <tbody>
    <% @timings.each do |timing| %>
      <tr>
        <td><%= DateTime.parse(timing.date.to_s).strftime("%m/%d/%Y")%></td>
        <td><%= timing.day%></td>
        <td><%= timing.hours + ":" + timing.minutes + " " + timing.ampm + " - " + (Time.strptime(timing.hours+timing.minutes+timing.ampm,"%I%M%P")+1.hours).strftime("%I:%M %P") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
